{
  "name": "odyssey",
  "version": "1.0.0",
  "description": "An all purpose Discord bot.",
  "repository": "https://github.com/rodrigoaddor/odyssey",
  "author": "Rodrigo Addor <rodrigotladdor@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "cross-env NODE_ENV=production node -r dotenv/config out/main.js",
    "build": "tsc",
    "dev": "ts-node-dev --inspect --respawn -r dotenv/config ./src/main.ts",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "postinstall": "husky install"
  },
  "dependencies": {
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "husky": "^5.0.6",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "^15.4",
    "yarn": "^1.22",
    "npm": "use-yarn"
  }
}
